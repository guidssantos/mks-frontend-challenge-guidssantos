{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
        "types": ["jest", "node", "@testing-library/jest-dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
        "allowSyntheticDefaultImports": true,
    "target": "ES5",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Sidebar/CardSidebar/CardSidebar.test.js"],
  "exclude": ["node_modules"]
}
